{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["code", "hooks"],
    "excludePattern": "(node_modules/|docs)",
    "includePattern": ".js$",
    "exclude": [
      "node_modules",
      "plugins"
    ]
  },
  "opts": {
    "template": "./node_modules/docdash",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "package": "./package.json",
    "readme": "./README.md",
    "private": true,
    "verbose": true
  },
  "markdown": {
    "hardwrap": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
